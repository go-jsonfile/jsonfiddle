package main

type Goods struct {
	Expensive int64 `json:"expensive"`
	Store     struct {
		Bicycle struct {
			Color string  `json:"color"`
			Price float64 `json:"price"`
		} `json:"bicycle"`
		Book []struct {
			Author   string  `json:"author"`
			Category string  `json:"category"`
			Isbn     string  `json:"isbn"`
			Price    float64 `json:"price"`
			Title    string  `json:"title"`
		} `json:"book"`
	} `json:"store"`
}

